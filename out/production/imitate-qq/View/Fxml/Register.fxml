<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="611.0" prefWidth="600.0" stylesheets="@CSS/Register.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.RegisterController">
    <children>
        <Label layoutX="26.0" layoutY="23.0" prefHeight="62.0" prefWidth="282.0" text="Personal Information:">
            <font>
                <Font name="Segoe Script" size="21.0" />
            </font>
        </Label>
        <TextField id="account" fx:id="idText" layoutX="26.0" layoutY="104.0" onKeyPressed="#enterId" prefHeight="42.0" prefWidth="247.0" promptText="请输入昵称" style="-fx-padding-left: 30;">
            <padding>
                <Insets left="30.0" />
            </padding></TextField>
        <TextField id="name" fx:id="nameText" layoutX="26.0" layoutY="172.0" onKeyPressed="#enterName" prefHeight="42.0" prefWidth="247.0" promptText="请输入姓名">
            <padding>
                <Insets left="30.0" />
            </padding></TextField>
        <RadioButton id="man" fx:id="btnMale" layoutX="77.0" layoutY="421.0" mnemonicParsing="false" onKeyPressed="#maleBtnPressed" prefHeight="23.0" prefWidth="50.0" selected="true" text="男" />
        <RadioButton id="women" fx:id="btnFemale" layoutX="155.0" layoutY="421.0" mnemonicParsing="false" onKeyPressed="#femaleBtnPressed" prefHeight="23.0" prefWidth="50.0" text="女" />
        <TextField id="phone" fx:id="phoneText" layoutX="40.0" layoutY="466.0" onKeyPressed="#enterPhone" prefHeight="42.0" prefWidth="203.0" promptText="请输入手机号">
            <padding>
                <Insets left="30.0" />
            </padding></TextField>
        <Button id="ChooseHead" layoutX="364.0" layoutY="361.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="126.0" text="选择头像" textFill="WHITE" />
        <Button id="back" layoutX="102.0" layoutY="557.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="78.0" text="取消" />
        <Button id="register" layoutX="359.0" layoutY="557.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="78.0" text="注册" />
        <Label id="accountError" layoutX="318.0" layoutY="144.0" prefHeight="20.0" prefWidth="216.0" textFill="RED">
            <font>
                <Font size="12.0" />
            </font>
        </Label>
        <Label id="nameError" layoutX="51.0" layoutY="183.0" prefHeight="20.0" prefWidth="216.0" textFill="RED">
            <font>
                <Font size="12.0" />
            </font>
        </Label>
        <Label id="passwordError" layoutX="316.0" layoutY="296.0" prefHeight="20.0" prefWidth="216.0" textFill="RED">
            <font>
                <Font size="12.0" />
            </font>
        </Label>
        <Label id="rePasswordError" layoutX="51.0" layoutY="324.0" prefHeight="20.0" prefWidth="216.0" textFill="RED">
            <font>
                <Font size="12.0" />
            </font>
        </Label>
        <Label id="phoneError" layoutX="51.0" layoutY="508.0" prefHeight="20.0" prefWidth="179.0" textFill="RED">
            <font>
                <Font size="12.0" />
            </font>
        </Label>
        <PasswordField id="password" fx:id="passwordText" layoutX="26.0" layoutY="238.0" onKeyPressed="#enterPassword" prefHeight="42.0" prefWidth="247.0" promptText="请输入密码">
            <padding>
                <Insets left="30.0" />
            </padding></PasswordField>
        <PasswordField id="rePassword" fx:id="rePasswordText" layoutX="26.0" layoutY="303.0" onKeyPressed="#reEnterPassword" prefHeight="42.0" prefWidth="247.0" promptText="请确认密码">
            <padding>
                <Insets left="30.0" />
            </padding></PasswordField>
        <Button id="HeadPortrait" layoutX="326.0" layoutY="115.0" mnemonicParsing="false" prefHeight="199.0" prefWidth="203.0" />
        <TextField id="codeText" fx:id="safeCodeText" layoutX="336.0" layoutY="466.0" prefHeight="42.0" prefWidth="143.0" promptText="验证码">
            <padding>
                <Insets left="30.0" />
            </padding>
        </TextField>
        <Button id="code" layoutX="480.0" layoutY="469.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="104.0" text="获取验证码" textFill="#4c0768">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <DatePicker id="birthday" layoutX="26.0" layoutY="367.0" prefHeight="36.0" prefWidth="247.0" promptText="请选择生日">
            <padding>
                <Insets left="20.0" />
            </padding>
        </DatePicker>
        <Label id="codeError" layoutX="300.0" layoutY="508.0" prefHeight="20.0" prefWidth="179.0" textFill="RED">
            <font>
                <Font size="12.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
